urlagent_paid = '<?php echo $this->url(array('module'=>'bookings','controller'=>'driverpayments','action'=>'get-agentcy-paid'));?>';
function getAgencyPaid(){
	dijit.byId("balance").attr("value",0);
	var type=dijit.byId("payment_by").get("value");
	//alert(type);
	if(type==1){
		var id = dijit.byId('invoice').get('value');
	}else{
		var id = dijit.byId('driver').get('value');
	}
	 
	var identity = $('#row_id').val();
	 
	if(identity==''){
		dijit.byId("total_commission_fee").attr("value",0);
		dijit.byId("total_alls").attr("value",0);
		dijit.byId("agency_paid").attr("value",0);
		//dijit.byId("agency_balance").attr("value",0);
		dijit.byId("total_alls").attr("value",0);
		dijit.byId("paid_agen").attr("value",0);
		dijit.byId("total_agen_recived").attr("value",0);
		dijit.byId("total_expense").attr("value",0);
		dijit.byId("balance").attr("value",0);
	}
	dojo.xhrPost({
	    url: urlagent_paid,
	    content:{
	     'driver':id,
	     'row_id':identity,
	     'type':type
	     },
	    handleAs:"json",
	    load: function(data) {
	    	//alert(data);
		   if(data){
			total_agency=Number(data.driver_paid);
			total_profit=Number(dijit.byId('total_alls').get("value"))-Number(dijit.byId('total_commission_fee').get("value"));
			dijit.byId("profit").attr("value",total_profit);
	    	dijit.byId("agency_paid").attr("value",data.driver_paid);
			//dijit.byId("agency_balance").attr("value",data.driver_balance);
			dijit.byId("total_agen_recived").attr("value",total_agency);
			//dijit.byId("paid_agen").attr("value",total_agency);
			var total_driver_fee=Number(dijit.byId("total_commission_fee").get("value"))+Number(dijit.byId("total_expense").get("value"));
			if(  total_driver_fee  < dijit.byId("total_agen_recived").get("value")){
				 $('#lb_name_agency').text("<?php echo $tr->translate("Driver Paid");?>");
				 dijit.byId("paid_agen").attr("value",Number(dijit.byId("total_agen_recived").get("value"))-Number(total_driver_fee));
				 dijit.byId("paid_type").attr("value",1);
		    }else{
		    	$('#lb_name_agency').text("<?php echo $tr->translate("Payment To Driver");?>");
		    	dijit.byId("paid_agen").attr("value",Number(total_driver_fee)-Number(dijit.byId("total_agen_recived").get("value")));
		    	dijit.byId("paid_type").attr("value",2);
		    }
		   } 
	    },
	    error: function(err) {
	    // alert(err);
	    }
	   });
}


function CalcualteDriverPaid(){
	dijit.byId("balance").attr("value",0);
	var total_driver_fee=0;
	var total_profit=0;
	    total_driver_fee=Number(dijit.byId("total_commission_fee").get("value"))+Number(dijit.byId("total_expense").get("value"));
	if(  total_driver_fee  < dijit.byId("total_agen_recived").get("value")){
		 $('#lb_name_agency').text("<?php echo $tr->translate("Driver Paid");?>");
		 dijit.byId("paid_agen").attr("value",Number(dijit.byId("total_agen_recived").get("value"))-Number(total_driver_fee));
		 dijit.byId("paid_type").attr("value",1);
    }else{
    	$('#lb_name_agency').text("<?php echo $tr->translate("Payment To Driver");?>");
    	dijit.byId("paid_agen").attr("value",Number(total_driver_fee)-Number(dijit.byId("total_agen_recived").get("value")));
    	dijit.byId("paid_type").attr("value",2);
    }

	total_profit=Number(dijit.byId('total_alls').get("value"))-Number(dijit.byId('total_commission_fee').get("value"));
	dijit.byId("profit").attr("value",total_profit);
}

function calCulatePrice(){
	var total_driver_fee=Number(dijit.byId("total_commission_fee").get("value"))+Number(dijit.byId("total_expense").get("value"));
	var total_age_re=dijit.byId("total_agen_recived").get("value");
	var rs=0;
	if(total_driver_fee<total_age_re){
		rs=Number(total_age_re)-Number(total_driver_fee);
	}else{
		rs=Number(total_driver_fee)-Number(total_age_re);
	}
	dijit.byId("balance").attr("value",rs);
	if(dijit.byId("balance").get("value")<dijit.byId("paid_agen").get("value")){
		//alert("Price Cannot Bigger Then Total Paid");
		//dijit.byId("paid_agen").attr("value",0);
		dijit.byId("balance").attr("value",Number(dijit.byId("balance").get("value"))-Number(dijit.byId("paid_agen").get("value")));
		return false;
	}else{
		dijit.byId("balance").attr("value",Number(dijit.byId("balance").get("value"))-Number(dijit.byId("paid_agen").get("value")));
	}
}

function convert(str) {
    var date = new Date(str),
        mnth = ("0" + (date.getMonth()+1)).slice(-2),
        day  = ("0" + date.getDate()).slice(-2);
    return [ date.getFullYear(), mnth, day ].join("-");
}




